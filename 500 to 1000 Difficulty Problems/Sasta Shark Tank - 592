import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
    public static void main(String[] args) throws java.lang.Exception
    {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        for (int i = 0; i < t; i++) {
            int x = sc.nextInt();
            int y = sc.nextInt();
            int valuationFirst = x* 10;
            int valuationSecond = y* 5;

            if (valuationFirst > valuationSecond) {
                System.out.println("FIRST");
            } else if (valuationFirst < valuationSecond) {
                System.out.println("SECOND");
            } else {
                System.out.println("ANY");
            }
        }
    }

}
